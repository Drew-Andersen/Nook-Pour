{
  "name": "nook-pour",
  "version": "1.0.0",
  "description": "Cocktail making App",
  "main": "server/app.py",
  "scripts": {
    "start": "cd server && python app.py",
    "develop": "concurrently \"cd server && python app.py\" \"cd client && npm run dev\"",
    "install": "cd server && pip install -r requirements.txt && cd ../client && npm i",
    "build": "cd client && npm run build",
    "setup-db": "mysql -u root -p < server/schema.sql",
    "render-build": "npm install && npm run build"
  },
  "keywords": [],
  "author": "Drew Andersen",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "license": "ISC"
}
